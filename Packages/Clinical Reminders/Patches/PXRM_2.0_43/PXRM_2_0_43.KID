"DATA",811.8,295,100,548,0)
UPDATE(DFN,VISIT,SOURCE,STCODE,TYPE) ;Make an update to the MST History file.
"DATA",811.8,295,100,549,0)
 N DATE,MSTDATE,PROV,STAT,TEMP,UPDSTAT,VPRVIEN
"DATA",811.8,295,100,550,0)
 S UPDSTAT=-1
"DATA",811.8,295,100,551,0)
 ;If the update is because of a protocol event use NOW for the
"DATA",811.8,295,100,552,0)
 ;date/time. If it is being done as part of a synchronization use
"DATA",811.8,295,100,553,0)
 ;the date the visit was created.
"DATA",811.8,295,100,554,0)
 S DATE=$S(TYPE="PROTOCOL":$$NOW^XLFDT,1:$P($G(^AUPNVSIT(VISIT,0)),U,2))
"DATA",811.8,295,100,555,0)
 ;If the date does not contain the time use noon.
"DATA",811.8,295,100,556,0)
 I DATE'["." S DATE=DATE_".12"
"DATA",811.8,295,100,557,0)
 S STAT=$$GETSTAT^DGMSTAPI(DFN)
"DATA",811.8,295,100,558,0)
 S MSTDATE=$S($P(STAT,U,1)>0:$P(STAT,U,3),1:0)
"DATA",811.8,295,100,559,0)
 I DATE>MSTDATE D
"DATA",811.8,295,100,560,0)
 .;Determine the provider.
"DATA",811.8,295,100,561,0)
 . S TEMP=$P(SOURCE,";",2)_$P(SOURCE,";",1)_",12)"
"DATA",811.8,295,100,562,0)
 . S PROV=$P($G(@TEMP),U,4)
"DATA",811.8,295,100,563,0)
 . I PROV="" D
"DATA",811.8,295,100,564,0)
 ..;DBIA #2316
"DATA",811.8,295,100,565,0)
 .. S VPRVIEN=+$O(^AUPNVPRV("AD",VISIT,""))
"DATA",811.8,295,100,566,0)
 .. I VPRVIEN>0 S PROV=$P(^AUPNVPRV(VPRVIEN,0),U,1)
"DATA",811.8,295,100,567,0)
 . S UPDSTAT=$$NEWSTAT^DGMSTAPI(DFN,STCODE,DATE,PROV)
"DATA",811.8,295,100,568,0)
 . I +UPDSTAT=-1 D
"DATA",811.8,295,100,569,0)
 .. N FN,GBL,IEN,NAME,TARGET,XMSUB,VADM
"DATA",811.8,295,100,570,0)
 .. K ^TMP("PXRMXMZ",$J)
"DATA",811.8,295,100,571,0)
 .. S XMSUB="CLINICAL REMINDER MST UPDATE PROBLEM"
"DATA",811.8,295,100,572,0)
 .. S ^TMP("PXRMXMZ",$J,1,0)="NEWSTAT^DGMSTAPI returned the following error:"
"DATA",811.8,295,100,573,0)
 .. S ^TMP("PXRMXMZ",$J,2,0)=$P(UPDSTAT,U,2)
"DATA",811.8,295,100,574,0)
 .. S ^TMP("PXRMXMZ",$J,3,0)="The following data was passed to NEWSTAT^DGMSTAPI"
"DATA",811.8,295,100,575,0)
 .. S ^TMP("PXRMXMZ",$J,4,0)="DFN = "_DFN
"DATA",811.8,295,100,576,0)
 .. S ^TMP("PXRMXMZ",$J,5,0)="Status code = "_STCODE
"DATA",811.8,295,100,577,0)
 .. S ^TMP("PXRMXMZ",$J,6,0)="Date = "_DATE
"DATA",811.8,295,100,578,0)
 .. S ^TMP("PXRMXMZ",$J,7,0)="Provider = "_PROV
"DATA",811.8,295,100,579,0)
 .. S ^TMP("PXRMXMZ",$J,8,0)="Data source = "_SOURCE
"DATA",811.8,295,100,580,0)
 .. S ^TMP("PXRMXMZ",$J,9,0)="This corresponds to the following:"
"DATA",811.8,295,100,581,0)
 .. D DEM^VADPT
"DATA",811.8,295,100,582,0)
 .. S ^TMP("PXRMXMZ",$J,10,0)="Patient = "_VADM(1)
"DATA",811.8,295,100,583,0)
 .. S ^TMP("PXRMXMZ",$J,11,0)="SSN = "_$P(VADM(2),U,2)
"DATA",811.8,295,100,584,0)
 .. S ^TMP("PXRMXMZ",$J,12,0)="MST Status = "_$$EXTERNAL^DILFD(29.11,3,"",STCODE)
"DATA",811.8,295,100,585,0)
 .. S ^TMP("PXRMXMZ",$J,13,0)="Date = "_$$FMTE^XLFDT(DATE,"5Z")
"DATA",811.8,295,100,586,0)
 .. S TEMP=$S(PROV="":"Unknown",1:TEMP=$$GET1^DIQ(200,PROV,.01,"","",""))
"DATA",811.8,295,100,587,0)
 .. I TEMP="" S TEMP="Unknown"
"DATA",811.8,295,100,588,0)
 .. S ^TMP("PXRMXMZ",$J,14,0)="Provider = "_TEMP
"DATA",811.8,295,100,589,0)
 .. S GBL=$P($P(SOURCE,";",2),"(",1)
"DATA",811.8,295,100,590,0)
 .. S TEMP=GBL_"(0)"
"DATA",811.8,295,100,591,0)
 .. S FN=+$P(@TEMP,U,2)
"DATA",811.8,295,100,592,0)
 .. S TEMP=GBL_"("_$P(SOURCE,";",1)_",0)"
"DATA",811.8,295,100,593,0)
 .. S TEMP=$G(@TEMP)
"DATA",811.8,295,100,594,0)
 .. S IEN=$P(TEMP,U,1)
"DATA",811.8,295,100,595,0)
 .. D FIELD^DID(FN,.01,"N","POINTER","TARGET")
"DATA",811.8,295,100,596,0)
 .. S GBL="^"_$P(TARGET("POINTER"),"(",1)
"DATA",811.8,295,100,597,0)
 .. S TEMP=GBL_"(0)"
"DATA",811.8,295,100,598,0)
 .. S FN=$P(@TEMP,U,1)
"DATA",811.8,295,100,599,0)
 .. S TEMP=GBL_"("_IEN_",0)"
"DATA",811.8,295,100,600,0)
 .. S NAME=$P(@TEMP,U,1)
"DATA",811.8,295,100,601,0)
 .. S ^TMP("PXRMXMZ",$J,14,0)="Data type = "_FN
"DATA",811.8,295,100,602,0)
 .. S ^TMP("PXRMXMZ",$J,15,0)="Name = "_NAME
"DATA",811.8,295,100,603,0)
 .. D SEND^PXRMMSG("PXRMXMZ",XMSUB,"",DUZ)
"DATA",811.8,295,100,604,0)
 Q UPDSTAT
"DATA",811.8,295,100,605,0)
