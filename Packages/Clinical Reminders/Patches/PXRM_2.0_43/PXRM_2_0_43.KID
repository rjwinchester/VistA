"DATA",811.8,295,100,424,0)
GSYINFO(TYPE) ;Return the Clinical Reminders MST synchronization date
"DATA",811.8,295,100,425,0)
 ;and the number of updates made. The format is an up-arrow delimited
"DATA",811.8,295,100,426,0)
 ;string. The first piece is the date and the second is the number
"DATA",811.8,295,100,427,0)
 ;of updates. If TYPE is "I" then the data for the initial
"DATA",811.8,295,100,428,0)
 ;synchronization is returned. For any other value the data for the
"DATA",811.8,295,100,429,0)
 ;last daily synchronization is returned.
"DATA",811.8,295,100,430,0)
 I $G(TYPE)="I" Q $P($G(^PXRM(800,1,"MST")),U,1,2) Q
"DATA",811.8,295,100,431,0)
 Q $P($G(^PXRM(800,1,"MST")),U,3,4)
"DATA",811.8,295,100,432,0)
 ;
"DATA",811.8,295,100,433,0)
 ;====================================================
"DATA",811.8,295,100,434,0)
QUE ;Queue the MST synchronization job.
"DATA",811.8,295,100,435,0)
 N DIR,DIROUT,DIRUT,DTOUT,DUOUT,MINDT,SDTIME,STIME,X,Y
"DATA",811.8,295,100,436,0)
 S MINDT=$$NOW^XLFDT
"DATA",811.8,295,100,437,0)
 W !,"Queue the Clinical Reminders MST synchronization."
"DATA",811.8,295,100,438,0)
 S DIR("A",1)="Enter the date and time you want the job to start."
"DATA",811.8,295,100,439,0)
 S DIR("A",2)="It must be after "_$$FMTE^XLFDT(MINDT,"5Z")
"DATA",811.8,295,100,440,0)
 S DIR("A")="Start the task at: "
"DATA",811.8,295,100,441,0)
 S DIR(0)="DAU"_U_MINDT_"::RSX"
"DATA",811.8,295,100,442,0)
 D ^DIR
"DATA",811.8,295,100,443,0)
 I $D(DIROUT)!$D(DIRUT) Q
"DATA",811.8,295,100,444,0)
 I $D(DTOUT)!$D(DUOUT) Q
"DATA",811.8,295,100,445,0)
 S SDTIME=Y
"DATA",811.8,295,100,446,0)
 K DIR
"DATA",811.8,295,100,447,0)
 S DIR(0)="YA"
"DATA",811.8,295,100,448,0)
 S DIR("A")="Do you want to run the MST synchronization at the same time every day? "
"DATA",811.8,295,100,449,0)
 S DIR("B")="Y"
"DATA",811.8,295,100,450,0)
 D ^DIR
"DATA",811.8,295,100,451,0)
 I $D(DIROUT)!$D(DIRUT) Q
"DATA",811.8,295,100,452,0)
 I $D(DTOUT)!$D(DUOUT) Q
"DATA",811.8,295,100,453,0)
 S STIME=$S(Y:"1."_$P(SDTIME,".",2),1:-1)
"DATA",811.8,295,100,454,0)
 ;
"DATA",811.8,295,100,455,0)
 ;Put the task into the queue.
"DATA",811.8,295,100,456,0)
 K ZTSAVE
"DATA",811.8,295,100,457,0)
 S ZTSAVE("STIME")=STIME
"DATA",811.8,295,100,458,0)
 S ZTRTN="SYNCH^PXRMMST"
"DATA",811.8,295,100,459,0)
 S ZTDESC="Clinical Reminders MST synchronization job"
"DATA",811.8,295,100,460,0)
 S ZTDTH=SDTIME
"DATA",811.8,295,100,461,0)
 S ZTIO=""
"DATA",811.8,295,100,462,0)
 D ^%ZTLOAD
"DATA",811.8,295,100,463,0)
 W !,"Task number ",ZTSK," queued."
"DATA",811.8,295,100,464,0)
 Q
"DATA",811.8,295,100,465,0)
 ;
"DATA",811.8,295,100,466,0)
 ;====================================================
"DATA",811.8,295,100,467,0)
STATUS(DFN,TEST,DATE,VALUE,TEXT) ;Computed finding for checking a
"DATA",811.8,295,100,468,0)
 ;patient's MST status.
"DATA",811.8,295,100,469,0)
 N IEN,TEMP
"DATA",811.8,295,100,470,0)
 S TEMP=$$GETSTAT^DGMSTAPI(DFN)
"DATA",811.8,295,100,471,0)
 S IEN=$P(TEMP,U,1)
"DATA",811.8,295,100,472,0)
 I IEN=-1 D  Q
"DATA",811.8,295,100,473,0)
 . S TEST=0,VALUE="",DATE=$$NOW^PXRMDATE
"DATA",811.8,295,100,474,0)
 I IEN=0 D  Q
"DATA",811.8,295,100,475,0)
 . S TEST=0
"DATA",811.8,295,100,476,0)
 . S VALUE=$P(TEMP,U,2)
"DATA",811.8,295,100,477,0)
 . S DATE=$P(TEMP,U,3)
"DATA",811.8,295,100,478,0)
 . S TEXT="No MST status found"
"DATA",811.8,295,100,479,0)
 ;If we get to here then a valid entry was found.
"DATA",811.8,295,100,480,0)
 S TEST=1
"DATA",811.8,295,100,481,0)
 S VALUE=$P(TEMP,U,2)
"DATA",811.8,295,100,482,0)
 S DATE=$P(TEMP,U,3)
"DATA",811.8,295,100,483,0)
 Q
"DATA",811.8,295,100,484,0)
