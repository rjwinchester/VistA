"DATA",811.8,295,100,491,0)
 ;====================================================
"DATA",811.8,295,100,492,0)
SYNCH ;Synchronize the MST history file.
"DATA",811.8,295,100,493,0)
 N INID,LTIME,NUMUPD,START,TEMP
"DATA",811.8,295,100,494,0)
 ;STIME is passed from QUE via ZTSAVE.
"DATA",811.8,295,100,495,0)
 D UPDSTAT(.NUMUPD,.START)
"DATA",811.8,295,100,496,0)
 ;If the initial sync data has been stored then update the daily
"DATA",811.8,295,100,497,0)
 ;data.
"DATA",811.8,295,100,498,0)
 S INID=+$P($G(^PXRM(800,1,"MST")),U,1)
"DATA",811.8,295,100,499,0)
 I INID>0 D
"DATA",811.8,295,100,500,0)
 . S $P(^PXRM(800,1,"MST"),U,3)=$$NOW^XLFDT
"DATA",811.8,295,100,501,0)
 . S $P(^PXRM(800,1,"MST"),U,4)=NUMUPD
"DATA",811.8,295,100,502,0)
 . S $P(^PXRM(800,1,"MST"),U,6)=START
"DATA",811.8,295,100,503,0)
 E  D
"DATA",811.8,295,100,504,0)
 . S $P(^PXRM(800,1,"MST"),U,1)=$$NOW^XLFDT
"DATA",811.8,295,100,505,0)
 . S $P(^PXRM(800,1,"MST"),U,2)=NUMUPD
"DATA",811.8,295,100,506,0)
 . S $P(^PXRM(800,1,"MST"),U,5)=START
"DATA",811.8,295,100,507,0)
 ;
"DATA",811.8,295,100,508,0)
 ;Cleanup the task stuff.
"DATA",811.8,295,100,509,0)
 I STIME=-1 S ZTREQ="@" Q
"DATA",811.8,295,100,510,0)
 E  D
"DATA",811.8,295,100,511,0)
 . S TEMP=$G(^PXRM(800,1,"MST"))
"DATA",811.8,295,100,512,0)
 . S LTIME=+$P(TEMP,U,3)
"DATA",811.8,295,100,513,0)
 . I LTIME=0 S LTIME=+$P(TEMP,U,1)
"DATA",811.8,295,100,514,0)
 .;Adding STIME sets the new starting time at exactly one day following
"DATA",811.8,295,100,515,0)
 .;the previous starting time.
"DATA",811.8,295,100,516,0)
 . S $P(ZTREQ,U,1)=$P(LTIME,".",1)+STIME
"DATA",811.8,295,100,517,0)
 Q
"DATA",811.8,295,100,518,0)
 ;
"DATA",811.8,295,100,519,0)
 ;====================================================
"DATA",811.8,295,100,520,0)
SYNREP ;Provide a report of the synchronization data.
"DATA",811.8,295,100,521,0)
 N EDTIME,EITIME,IDATE,LDATE,NIUPD,NLUPD,TEMP
"DATA",811.8,295,100,522,0)
 S TEMP=$G(^PXRM(800,1,"MST"))
"DATA",811.8,295,100,523,0)
 S IDATE=$$FMTE^XLFDT($P(TEMP,U,1))
"DATA",811.8,295,100,524,0)
 I IDATE=0 S IDATE="none"
"DATA",811.8,295,100,525,0)
 S NIUPD=$P(TEMP,U,2)
"DATA",811.8,295,100,526,0)
 S EITIME=$$FMDIFF^XLFDT($P(TEMP,U,1),$P(TEMP,U,5),2)
"DATA",811.8,295,100,527,0)
 S LDATE=$$FMTE^XLFDT($P(TEMP,U,3))
"DATA",811.8,295,100,528,0)
 I LDATE=0 S LDATE="none"
"DATA",811.8,295,100,529,0)
 S NLUPD=$P(TEMP,U,4)
"DATA",811.8,295,100,530,0)
 S EDTIME=$$FMDIFF^XLFDT($P(TEMP,U,3),$P(TEMP,U,6),2)
"DATA",811.8,295,100,531,0)
 W !!,"Clinical Reminders MST Synchronization Report"
"DATA",811.8,295,100,532,0)
 W !,"---------------------------------------------"
"DATA",811.8,295,100,533,0)
 W !,"Initial synchronization date: ",IDATE
"DATA",811.8,295,100,534,0)
 W !,"Number of updates made: ",NIUPD
"DATA",811.8,295,100,535,0)
 I EITIME>60 D
"DATA",811.8,295,100,536,0)
 . S EITIME=$$FMDIFF^XLFDT($P(TEMP,U,1),$P(TEMP,U,5),3)
"DATA",811.8,295,100,537,0)
 . W !,"Elapsed time: ",EITIME
"DATA",811.8,295,100,538,0)
 E  W !,"Elapsed time: ",EITIME," secs"
"DATA",811.8,295,100,539,0)
 W !!,"Last daily synchronization date: ",LDATE
"DATA",811.8,295,100,540,0)
 W !,"Number of updates made: ",NLUPD
"DATA",811.8,295,100,541,0)
 I EDTIME>60 D
"DATA",811.8,295,100,542,0)
 . S EDTIME=$$FMDIFF^XLFDT($P(TEMP,U,3),$P(TEMP,U,6),3)
"DATA",811.8,295,100,543,0)
 . W !,"Elapsed time: ",EDTIME
"DATA",811.8,295,100,544,0)
 E  W !,"Elapsed time: ",EDTIME," secs"
"DATA",811.8,295,100,545,0)
 Q
"DATA",811.8,295,100,546,0)
