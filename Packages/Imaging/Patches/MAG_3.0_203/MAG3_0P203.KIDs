 ; There are no environment checks here but the MAGIP203 has to be
"RTN","MAGIP203",19,0)
 ; referenced by the "Environment Check Routine" field of the KIDS
"RTN","MAGIP203",20,0)
 ; build so that entry points of the routine are available to the
"RTN","MAGIP203",21,0)
 ; KIDS during all installation phases.
"RTN","MAGIP203",22,0)
 Q
"RTN","MAGIP203",23,0)
 ;
"RTN","MAGIP203",24,0)
 ;+++++ INSTALLATION ERROR HANDLING
"RTN","MAGIP203",25,0)
ERROR ;
"RTN","MAGIP203",26,0)
 S:$D(XPDNM) XPDABORT=1
"RTN","MAGIP203",27,0)
 ;--- Display the messages and store them to the INSTALL file
"RTN","MAGIP203",28,0)
 D DUMP^MAGUERR1(),ABTMSG^MAGKIDS()
"RTN","MAGIP203",29,0)
 Q
"RTN","MAGIP203",30,0)
 ;
"RTN","MAGIP203",31,0)
 ;***** POST-INSTALL CODE
"RTN","MAGIP203",32,0)
POS ;
"RTN","MAGIP203",33,0)
 N CALLBACK
"RTN","MAGIP203",34,0)
 D CLEAR^MAGUERR(1)
"RTN","MAGIP203",35,0)
 ;
"RTN","MAGIP203",36,0)
 ;--- Send the notification e-mail
"RTN","MAGIP203",37,0)
 D BMES^XPDUTL("Post Install Mail Message: "_$$FMTE^XLFDT($$NOW^XLFDT))
"RTN","MAGIP203",38,0)
 D INS^MAGQBUT4(XPDNM,DUZ,$$NOW^XLFDT,XPDA)
"RTN","MAGIP203",39,0)
 Q
"RTN","MAGIP203",40,0)
 ;
"RTN","MAGIP203",41,0)
 ;***** PRE-INSTALL CODE
"RTN","MAGIP203",42,0)
PRE ;
"RTN","MAGIP203",43,0)
 Q
"VER")
8.0^22.2
**END**
**END**
