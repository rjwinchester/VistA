 ;                Add a new entry if one is not found
"RTN","EASECU2",50,0)
 ;                 Input  -- DFN    Patient file IEN
"RTN","EASECU2",51,0)
 ;                           DGINI  Individual Annual Income IEN
"RTN","EASECU2",52,0)
 ;                 Output -- Income Relation IEN
"RTN","EASECU2",53,0)
 N DGIRI
