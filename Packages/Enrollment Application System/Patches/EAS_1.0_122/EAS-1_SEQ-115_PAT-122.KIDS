 ;
"RTN","EASECU2",4,0)
GETIENS(DFN,DGPRI,DGDT) ;Look-up individual annual income and income relation
"RTN","EASECU2",5,0)
 ;                 Input  -- DFN    Patient file IEN
"RTN","EASECU2",6,0)
 ;                           DGPRI  Patient Relation IEN
"RTN","EASECU2",7,0)
